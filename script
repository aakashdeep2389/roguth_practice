// [2,4,7,3,1,3,6] input,  create a new array where sum all elements except current index 

// example if current item is 2 then except 2 sum all and place in first position of array and then next javascript

const input = [2,4,7,3,1,3,6]
// let temp = 0
// const finaloutput = input.map((item, index)=>{
//     console.log(item);
//     return temp += item
// })
// console.log('**', finaloutput, temp);

let temp2 = 0
let arr = []
for(let i=0; i<input.length; i++){
    console.log(input[i]);
    for(let j=0; j<input.length; j++){
        if(input[i] !== input[j]){
            temp2 += input
        }
    }
    arr.push(temp2)
}
console.log(arr);

