//same object key as a new object
let obj1 = {
    a:1,
    b:2,
    c:3,
    d:4,
    e:7
}
let obj2 = {
    a:2,
    b:2,
    c:7,
    d:4,
    e:7
}

//ouput  { b:2,  d: 4}

function test(){
    let output = {}
for(let key in obj1) {
    for(let key2 in obj2) {
        if(obj1[key] == obj2[key2]){
            // Object.assign(output, { [key]: obj1[key] })
            output[key] = obj1[key]
        }
    }
}
}
console.log(output);


